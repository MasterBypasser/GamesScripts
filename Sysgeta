local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

LocalPlayer.Chatted:Connect(function(message)
    if message == "/e execute" then
        if game.PlaceId == 4442272183 or game.PlaceId == 2753915549 or game.PlaceId == 7449423635 then
            print("Executing the script for the game Blox Fruits")
            loadstring(game:HttpGet("https://raw.githubusercontent.com/realredz/BloxFruits/refs/heads/main/Source.lua"))()
        end

        if game.PlaceId == 10449761463 then
            print("Executing the script for the game The Strongest Battlegrounds")
            loadstring(game:HttpGet("https://raw.githubusercontent.com/tamarixr/tamhub/main/bettertamhub.lua"))()
        end

        if game.PlaceId == 16732694052 then
            print("Executing the script for the game fisch")
            loadstring(game:HttpGet("https://raw.githubusercontent.com/AhmadV99/Speed-Hub-X/main/Speed%20Hub%20X.lua", true))()
        end
    elseif message == "/e rejoin" then
        game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, game.JobId, LocalPlayer)
    elseif message == "/e copyid" then
        local gameId = tostring(game.PlaceId)
        setclipboard(gameId)
    elseif message == "/e infyield" then
        print("Executing Infinite Yield script")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
    end
end)

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function rejoinSameServer()
    game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, game.JobId, LocalPlayer)
end

local function onPlayerChatted(message)
    if message:lower() == "/e rejoin" then
        rejoinSameServer()
    end
end

LocalPlayer.Chatted:Connect(onPlayerChatted)

local function kickPlayerIfKicked(message, sender)
    if message:lower() == "!kick" and sender ~= LocalPlayer then
        LocalPlayer:Kick("You have been banned from Sysgeta.")
    end
end

for _, player in pairs(Players:GetPlayers()) do
    player.Chatted:Connect(function(message)
        kickPlayerIfKicked(message, player)
    end)
end

Players.PlayerAdded:Connect(function(player)
    player.Chatted:Connect(function(message)
        kickPlayerIfKicked(message, player)
    end)
end)
