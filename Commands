local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")
local LocalPlayer = Players.LocalPlayer

local function printCommandInfo(command, description)
    print(command .. ": " .. description)
end

LocalPlayer.Chatted:Connect(function(message)
    if message == "/e execute" then
        if game.PlaceId == 4442272183 or game.PlaceId == 2753915549 or game.PlaceId == 7449423635 then
            print("Executing the script for the game Blox Fruits")
            loadstring(game:HttpGet("https://raw.githubusercontent.com/realredz/BloxFruits/refs/heads/main/Source.lua"))()
        end

        if game.PlaceId == 10449761463 then
            print("Executing the script for the game The Strongest Battlegrounds")
            loadstring(game:HttpGet("https://raw.githubusercontent.com/tamarixr/tamhub/main/bettertamhub.lua"))()
        end

        if game.PlaceId == 16732694052 then
            print("Executing the script for the game fisch")
            loadstring(game:HttpGet("https://raw.githubusercontent.com/AhmadV99/Speed-Hub-X/main/Speed%20Hub%20X.lua", true))()
        end

        if game.PlaceId == 286090429 then
            print("Executing the script for the game QuotasHub")
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Insertl/QuotasHub/main/BETAv1.3"))()
        end
        printCommandInfo("/e execute", "Executes the game-specific script")
    elseif message == "/e copygameid" then
        local gameId = tostring(game.PlaceId)
        setclipboard(gameId)
        printCommandInfo("/e copygameid", "Copies the game's PlaceId to clipboard")
    elseif message == "/e copyserverid" then
        local serverId = tostring(game.JobId)
        setclipboard(serverId)
        printCommandInfo("/e copyserverid", "Copies the server's JobId to clipboard")
    elseif message:sub(1, 14) == "/e joinserver " then
        local serverId = message:sub(15)
        TeleportService:TeleportToPlaceInstance(game.PlaceId, serverId, LocalPlayer)
        printCommandInfo("/e joinserver", "Teleports to the specified server")
    elseif message == "/e infyield" then
        print("Executing Infinite Yield script")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
        printCommandInfo("/e infyield", "Executes the Infinite Yield script")
    end
end)

local function rejoinSameServer()
    TeleportService:TeleportToPlaceInstance(game.PlaceId, game.JobId, LocalPlayer)
end

local function onPlayerChatted(message)
    if message:lower() == "/e rejoin" then
        rejoinSameServer()
        printCommandInfo("/e rejoin", "Rejoins the same server")
    end
end

LocalPlayer.Chatted:Connect(onPlayerChatted)

local function kickPlayerIfKicked(message, sender)
    if message:lower() == "!ban ." and sender ~= LocalPlayer then
        LocalPlayer:Kick("You were kicked from this experience: Exploiting - Banned From Sysgeta")
    end
end

for _, player in pairs(Players:GetPlayers()) do
    player.Chatted:Connect(function(message)
        kickPlayerIfKicked(message, player)
    end)
end

Players.PlayerAdded:Connect(function(player)
    player.Chatted:Connect(function(message)
        kickPlayerIfKicked(message, player)
    end)
end)
